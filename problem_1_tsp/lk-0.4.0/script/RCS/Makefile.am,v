EXTRA_DIST = README  \
cd.pl.in \
improve.pl.in \
lkdoit.in \
lkdoitcmp.in \
milestone.pl.in \
number.pl.in \
probe.pl.in \
tspreorder.pl.in

bin_SCRIPTS = \
cd.pl \
improve.pl \
lkdoit \
lkdoitcmp \
milestone.pl \
number.pl \
probe.pl \
tspreorder.pl

# A hack to use the SCRIPTS before a full-blown install.
myall :
	chmod +x $(bin_SCRIPTS)

# Distribute RCS files too.
dist-hook :
	cp -r RCS $(distdir)
@


